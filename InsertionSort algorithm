
public class insertion {
    public static void main (String args[]){
      int arr[]={5,8,1,3,9,6};
      
      for(int i=1;i<arr.length;i++){
          
          int key=arr[i];
          int j=i-1;
          
          for( j=i-1 ; j>=0 && arr[j] > key  ;j--){
           int temp=arr[j];
           arr[j]=arr[j+1];
           arr[j+1]=temp;
           
          }
      }
       for(int x : arr){
            System.out.print(x);
    }


            
        }
    
    
}
